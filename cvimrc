"let barposition="bottom"
set noautofocus
unmap s
unmap x
unmap d
map s previousTab
map x nextTab
map d closeTab

let searchengine baidu = "https://www.baidu.com/s?wd=%s"
let searchengine bing = "https://www.bing.com/search?q=%s"

let completionengines = ["baidu", "bing"]
let defaultengine ="baidu"

let searchalias bd = "baidu"
let searchalias bi = "bing"
